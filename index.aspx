    <script type="text/javascript">
    function toggleDescription(link) {
        var description = link.previousSibling; // span containing full description
        var fullDesc = description.querySelector('.full-description');
        var readMoreLink = link;

        if (description.style.display === 'none' || description.style.display === '') {
            description.style.display = 'inline'; // Display the full description
            fullDesc.style.display = 'inline'; // Display the full description
            readMoreLink.innerHTML = 'Read less'; // Change link text to 'Read less'
        } else {
            description.style.display = 'inline'; // Display the full description
            fullDesc.style.display = 'none'; // Display the full description
            readMoreLink.innerHTML = 'Read more'; // Change link text to 'Read more'
        }
    }
    </script>

